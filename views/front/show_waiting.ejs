<div class="container px-3 m-auto">
  <div class="flex flex-col items-center justify-center p-4 text-white bg-gray-200 border shadow-lg rounded-xl">



    <% if (queues.length > 0) { %>
    <p class="text-xl font-bold text-primary"><%= queues.length %></p>
     
    <div class="flex items-center justify-center w-10 h-10 mb-5 bg-secondary rounded-2xl">
        <p><%= queues.length %>  * 15 </p>
        <p>Min</p>
    </div>
    <% } else { %>
    <p class="text-xl font-bold text-primary">No queues found.</p>
    <% } %>

     <form action="/book/my/turn/<%= place %>/<%= service  %>" method="post">
        <button class="btn btn-primary" >Book My Turn</button>
     </form>
    
  </div>
</div>